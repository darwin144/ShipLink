@using Client_Shiplink.DummyData
@using Client_Shiplink.Models.OrderDetails

@{
    Layout = "~/Views/Shared/_DashboardLayout.cshtml";
    var dummyData = DummyDataOrder.GetDummyData();
}
<head>
     <link href="~/css/dashboardIndex.css" rel="stylesheet" />
</head>

<div class="d-flex gap-2 w-100 h-100 " >
    <div class="border rounded w-100" style="padding: 20px ;">
        <div class=" d-flex justify-content-between align-items-center py-3 w-100 h-auto ">
        <div class=" d-flex  text-start " style="gap:20px;">
            <h1>Income Shipments</h1>
        </div>
       <div class="w-25 d-flex gap-2 ">
            <partial name="../Components/SearchIconComponents" />
            <partial name="../Components/SortComponents" />
        </div>
        </div>
        <div class="d-flex gap-3 flex-column">
    @foreach (var orderDetail in dummyData.Where(od => od.Information == "My Mailbox" || od.Information == "Shipping Mailbox"))
    {
        <partial name="../Components/OrderDetailComponents" model="orderDetail" />
    }
        </div>
    </div>
    <partial name="../Components/DashboardStatus" />
</div>





